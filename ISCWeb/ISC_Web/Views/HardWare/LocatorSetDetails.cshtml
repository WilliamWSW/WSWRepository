@model ISC_Web.Models.sys_LocatorSet
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.12.3.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <title>LocatorSetDetails</title>
</head>
<body>
    <div class="container-fluid">
        <h4 class="text-center"><strong>板锁和RFID详细信息</strong></h4>
        <hr />

        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 text-right">
                <strong>终端名称:</strong>
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3 text-left" id="lbllmterminal">
                @Html.DisplayFor(Model => Model.terminalID)
            </div>
            <div class="col-xs-1 col-sm-1 col-md-1 text-right">
                <strong>格口名称:</strong>
            </div>
            <div class="col-xs-5 col-sm-5 col-md-5 text-left" id="lbllmlocator">
                @Html.DisplayFor(Model => Model.locatorID)
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 text-right">
                <strong>唯一识别号:</strong>
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3 text-left" id="lbllmuniqueCode">
                @Html.DisplayFor(Model => Model.uniqueCode)
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 text-right">
                <strong>板地址:</strong>
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3 text-left" id="lbllmplateAddress">
                @Html.DisplayFor(model => model.plateAddress)
            </div>
            <div class="col-xs-1 col-sm-1 col-md-1 text-right">
                <strong>锁地址:</strong>
            </div>
            <div class="col-xs-5 col-sm-5 col-md-5 text-left" id="lbllmlockAddress">
                @Html.DisplayFor(model => model.lockAddress)
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 text-right">
                <strong>RFID天线编号:</strong>
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3 text-left" id="lbllmantennasID">
                @Html.DisplayFor(model => model.antennasID)
            </div>
            <div class="col-xs-1 col-sm-1 col-md-1 text-right">
                <strong>RFID天线IP地址:</strong>
            </div>
            <div class="col-xs-5 col-sm-5 col-md-5 text-left" id="lbllmRFIDIP">
                @Html.DisplayFor(model => model.RFIDIP)
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 text-right">
                <strong>RFID天线功率:</strong>
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3 text-left" id="lbllmantennasGL">
                @Html.DisplayFor(model => model.antennasGL)
            </div>
            <div class="col-xs-1 col-sm-1 col-md-1 text-right">
                <strong>RFID天线灵敏度:</strong>
            </div>
            <div class="col-xs-5 col-sm-5 col-md-5 text-left" id="lbllmantennasLMD">
                @Html.DisplayFor(model => model.antennasLMD)
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 text-right">
                <strong>编辑人员:</strong>
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3 text-left" id="lbllmupdateMan">
                @Html.DisplayFor(Model => Model.updateMan)
            </div>
            <div class="col-xs-1 col-sm-1 col-md-1 text-right">
                <strong>编辑时间:</strong>
            </div>
            <div class="col-xs-5 col-sm-5 col-md-5 text-left" id="lbllmupdateTime">
                @Html.DisplayFor(Model => Model.updateTime)
            </div>
        </div>
        <hr />
        <br /><br /><br /><br />
        <div style="position:fixed;height:90px;bottom:0px;">
            <hr />
            @Html.ActionLink("返  回", "LocatorSet",
       new
       {
           id = ViewBag.Screen,
           screen = ViewBag.Screen,
           Hisorder = ViewBag.Order,
           search = ViewBag.Search,
           filter = ViewBag.Filter,
           page = ViewBag.PageNum,
           goback = 1
       }, new { @class = "btn btn btn-default", @title = "取消操作,关闭弹出框", @id = "btnlmback" })
            <button class=" btn btn-default hidden data-toggle=" modal" data-target="#detailModal" id="btnlmup">
                返回上一级
            </button>

        </div>
    </div>


    <div class="modal fade" id="detailModal" tabindex="-1" role="dialog" aria-labelledby="返回确认" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;
                    </button>
                    <strong class="modal-title text-primary" id="myModalLabel">
                        操作确认
                    </strong>
                </div>
                <div class="modal-body ">
                    返回上一级界面吗?
                </div>
                <div class="modal-footer">

                    @Html.ActionLink("确 认", "LocatorSet",
       new
       {
           id = ViewBag.Screen,
           screen = ViewBag.Screen,
           Hisorder = ViewBag.Order,
           search = ViewBag.Search,
           filter = ViewBag.Filter,
           page = ViewBag.PageNum,
           goback = 1
       }, new { @class = "btn btn btn-default", @id = "btnlmsure" })

                    <button type="button" class="btn btn-default" id="btnlmcancle" data-dismiss="modal" data-toggle="tooltip" data-placement="top" title="取消操作,关闭弹出框">
                        取&nbsp;消
                    </button>
                </div>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->


    <script type="text/javascript">
        function centerModals() {
            $('#detailModal').each(function (i) {
                var $clone = $(this).clone().css('display', 'block').appendTo('body');
                var top = Math.round(($clone.height() - $clone.find('.modal-content').height()) / 2);
                top = top > 0 ? top : 0;
                $clone.remove();
                $(this).find('.modal-content').css("margin-top", top);
            });
        };
    </script>

    <script type="text/javascript">
        $(function () { $('#detailModal').on('show.bs.modal', centerModals) });

    </script>

</body>
</html>